#include <iostream>
using namespace std;


int main()
{
    string username, password;
    cout << "Enter username:";
    cin >> username;
    cout << "Enter password:";
    cin >> password;
    if (username == "Elijah"
        && password == "Elijah123") {
        cout << "Your are logged in.";
    }
    else if (username != "Elijah"
        && password == "Elijah123") {
        cout << "Your username is incorrect.";
    }
    else if (username == "Elijah"
        && password != "Elijah123") {
		cout << "Your password is incorrect.";
    }
    return 0;
   
}
